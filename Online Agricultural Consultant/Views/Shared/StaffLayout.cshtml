<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1,user-scalable=1.0">
    <title>Online Agricultural Consultant | Staff Dashboard</title>
    <link href="~/DashboardStyles/css/bootstrap.min.css" rel="stylesheet" type="text/css">
    <link href="~/DashboardStyles/css/londinium-theme.css" rel="stylesheet" type="text/css">
    <link href="~/DashboardStyles/css/styles.css" rel="stylesheet" type="text/css">
    <link href="~/DashboardStyles/css/icons.css" rel="stylesheet" type="text/css">
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700&amp;subset=latin,cyrillic-ext" rel="stylesheet" type="text/css">
    <script type="text/javascript" src="~/DashboardStyles/ajax.googleapis.com/ajax/libs/jquery/1.10.1/jquery.min.js"></script>
    <script type="text/javascript" src="~/DashboardStyles/ajax.googleapis.com/ajax/libs/jqueryui/1.10.2/jquery-ui.min.js"></script>
    <script type="text/javascript" src="~/DashboardStyles/js/plugins/charts/sparkline.min.js"></script>
    <script type="text/javascript" src="~/DashboardStyles/js/plugins/forms/uniform.min.js"></script>
    <script type="text/javascript" src="~/DashboardStyles/js/plugins/forms/select2.min.js"></script>
    <script type="text/javascript" src="~/DashboardStyles/js/plugins/forms/inputmask.js"></script>
    <script type="text/javascript" src="~/DashboardStyles/js/plugins/forms/autosize.js"></script>
    <script type="text/javascript" src="~/DashboardStyles/js/plugins/forms/inputlimit.min.js"></script>
    <script type="text/javascript" src="~/DashboardStyles/js/plugins/forms/listbox.js"></script>
    <script type="text/javascript" src="~/DashboardStyles/js/plugins/forms/multiselect.js"></script>
    <script type="text/javascript" src="~/DashboardStyles/js/plugins/forms/validate.min.js"></script>
    <script type="text/javascript" src="~/DashboardStyles/js/plugins/forms/tags.min.js"></script>
    <script type="text/javascript" src="~/DashboardStyles/js/plugins/forms/switch.min.js"></script>
    <script type="text/javascript" src="~/DashboardStyles/js/plugins/forms/uploader/plupload.full.min.js"></script>
    <script type="text/javascript" src="~/DashboardStyles/js/plugins/forms/uploader/plupload.queue.min.js"></script>
    <script type="text/javascript" src="~/DashboardStyles/js/plugins/forms/wysihtml5/wysihtml5.min.js"></script>
    <script type="text/javascript" src="~/DashboardStyles/js/plugins/forms/wysihtml5/toolbar.js"></script>
    <script type="text/javascript" src="~/DashboardStyles/js/plugins/interface/daterangepicker.js"></script>
    <script type="text/javascript" src="~/DashboardStyles/js/plugins/interface/fancybox.min.js"></script>
    <script type="text/javascript" src="~/DashboardStyles/js/plugins/interface/moment.js"></script>
    <script type="text/javascript" src="~/DashboardStyles/js/plugins/interface/jgrowl.min.js"></script>
    <script type="text/javascript" src="~/DashboardStyles/js/plugins/interface/datatables.min.js"></script>
    <script type="text/javascript" src="~/DashboardStyles/js/plugins/interface/colorpicker.js"></script>
    <script type="text/javascript" src="~/DashboardStyles/js/plugins/interface/fullcalendar.min.js"></script>
    <script type="text/javascript" src="~/DashboardStyles/js/plugins/interface/timepicker.min.js"></script>
    <script type="text/javascript" src="~/DashboardStyles/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/DashboardStyles/js/application.js"></script>
</head>
<body>
    <div class="navbar navbar-inverse" role="navigation">
        <div class="navbar-header">
            <a class="sidebar-toggle"><i class="icon-paragraph-justify2"></i></a>
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-icons">
                <span class="sr-only">Toggle navbar</span>
                <i class="icon-grid3"></i>
            </button>
            <button type="button" class="navbar-toggle offcanvas">
                <span class="sr-only">Toggle navigation</span>
                <i class="icon-paragraph-justify2"></i>
            </button>
        </div>
        <ul class="nav navbar-nav navbar-right collapse" id="navbar-icons">
            <li class="user dropdown">
                <a class="dropdown-toggle" data-toggle="dropdown">
                    <img src="images/demo/users/face1.png" alt="">
                    <span>Settings</span>
                    <i class="caret"></i>
                </a>
                <ul class="dropdown-menu dropdown-menu-right icons-right">
                    <li><a href="#"><i class="icon-user"></i> Profile</a></li>
                    <li><a href="#"><i class="icon-bubble4"></i> Enquery</a></li>
                    @*<li><a href="#"><i class="icon-cog"></i> Settings</a></li>*@
                    <li><a href="/Staff/Logout"><i class="icon-exit"></i> Logout</a></li>
                </ul>
            </li>
        </ul>
    </div>
    <div class="page-container">
        <div class="sidebar">
            <div class="sidebar-content">
                <!-- User dropdown -->
                <div class="user-menu dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                        <img src="images/demo/users/face3.png" alt="">
                        <div class="user-info">
                            Staff Panel<span></span>
                        </div>
                    </a>
                </div>
                <!-- /user staff_profile -->
                <!-- Main navigation -->
                <ul class="navigation">
                    <li class="active"><a href="/Staff/Dashboard"><span>Dashboard</span> <i class="icon-screen2"></i></a></li>
                    <li><a href="/Staff/staff_profile"><span>Profile</span> <i class="icon-user"></i></a></li>

                    <li>
                        <a href="#"><span>Manage Query</span> <i class="icon-paragraph-justify2"></i></a>
                        <ul>
                            <li><a href="/Staff/view_Query">View Query</a></li>
                            <li><a href="/Staff/view_solved_Query">View Solved issues</a></li>
                        </ul>
                    </li>
                    <li><a href="/Staff/Logout"><span>Logout</span> <i class="icon-exit"></i></a></li>

                </ul>
            </div>
        </div>
        @RenderBody()
    </div>
</body>
</html>
